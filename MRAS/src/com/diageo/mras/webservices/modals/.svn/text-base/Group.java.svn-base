package com.diageo.mras.webservices.modals;

import javax.xml.bind.annotation.XmlRootElement;

/**
 * This is a bean class for the Group entity.
 * 
 * @author Infosys Limited
 * @version 1.0
 */
@XmlRootElement
public class Group {
	private String appId;

	public String getAppId() {
		return appId;
	}

	public void setAppId(String appId) {
		this.appId = appId;
	}

	private int groupID;
	private String groupOwnerPhoneNumber;
	private String status;

	public String getGroupOwnerPhoneNumber() {
		return groupOwnerPhoneNumber;
	}

	public int getGroupID() {
		return groupID;
	}

	public void setGroupID(int groupID) {
		this.groupID = groupID;
	}

	public void setGroupOwnerPhoneNumber(String groupOwnerPhoneNumber) {
		this.groupOwnerPhoneNumber = groupOwnerPhoneNumber;
	}

	public String getStatus() {
		return status;
	}

	public void setStatus(String status) {
		this.status = status;
	}

	public String toString() {
		return "{GroupID: " + groupID + ", groupOwnerPhoneNumber:"
				+ groupOwnerPhoneNumber + "}";

	}

}
