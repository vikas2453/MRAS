package com.diageo.mras.webservices.modals;

import javax.xml.bind.annotation.XmlAccessType;
import javax.xml.bind.annotation.XmlAccessorType;
import javax.xml.bind.annotation.XmlElement;
import javax.xml.bind.annotation.XmlRootElement;

@XmlRootElement
@XmlAccessorType(XmlAccessType.FIELD)
public class OfferRedemptionStatusModal {
	@XmlElement(name = "maxRedemeption")
	private int maxRedemeption;
	@XmlElement(name = "redemptionTillNow")
	private int redemptionTillNow;

	public int getMaxRedemeption() {
		return maxRedemeption;
	}

	public void setMaxRedemeption(int maxRedemeption) {
		this.maxRedemeption = maxRedemeption;
	}

	public int getRedemptionTillNow() {
		return redemptionTillNow;
	}

	public void setRedemptionTillNow(int redemptionTillNow) {
		this.redemptionTillNow = redemptionTillNow;
	}

}
